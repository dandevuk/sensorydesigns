        "text_NrRnPc": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3><strong>Why Regular Cleaning Matters</strong></h3>",
            "width": "fit-content",
            "max_width": "normal",
            "alignment": "left",
            "type_preset": "rte",
            "font": "var(--font-body--family)",
